# Environment Configuration for Egyptian Legal Analyzer

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Bedrock Configuration
EGYPTIAN_LAW_KB_ID=your_knowledge_base_id_here
CONTRACT_ANALYSIS_AGENT_ID=your_contract_agent_id_here
COMPLIANCE_AGENT_ID=your_compliance_agent_id_here
RISK_ASSESSMENT_AGENT_ID=your_risk_agent_id_here

# S3 Buckets
CONTRACT_BUCKET=egyptian-legal-analyzer-contracts-ACCOUNT_ID
KB_BUCKET=egyptian-legal-analyzer-knowledge-base-ACCOUNT_ID
AUDIT_BUCKET=egyptian-legal-analyzer-audit-ACCOUNT_ID

# KMS Encryption
KMS_KEY_ID=alias/egyptian-legal-analyzer-key

# Security Configuration
ENCRYPTION_PASSWORD=your_secure_password_here
ENCRYPTION_SALT=your_secure_salt_here

# API Configuration
API_BASE_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/prod

# Database Configuration (if using RDS)
DATABASE_URL=postgresql://username:password@hostname:5432/database

# Application Settings
DEBUG=False
LOG_LEVEL=INFO
MAX_FILE_SIZE_MB=50
SESSION_TIMEOUT_MINUTES=60

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,https://your-frontend-domain.com

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100
RATE_LIMIT_BURST=20